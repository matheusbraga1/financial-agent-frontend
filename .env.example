# ============================================
# Financial Agent Frontend - Environment Variables
# ============================================
#
# Copie este arquivo para:
#   - .env (produção na VM)
#   - .env.development (desenvolvimento local)
#

# API Backend URL
# Desenvolvimento: http://localhost:8000/api/v1
# Produção: http://192.168.1.150/api/v1
VITE_API_URL=http://localhost:8000/api/v1

# Porta da aplicação (apenas dev server)
VITE_PORT=3000

# Habilitar logs no console
# Desenvolvimento: true
# Produção: false
VITE_ENABLE_LOGS=false

# Ambiente
VITE_ENV=development

# Versão da aplicação (opcional)
VITE_APP_VERSION=1.0.0
